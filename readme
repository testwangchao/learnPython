class Test():
    #静态方法
    @staticmethod
    def test(a,b):
        return a + b
    #类中静态方法的相互调用
    @staticmethod
    def test2(c,d):
        return Test.test(c, d)
    #动态方法
    @classmethod
    def test3(cls):
        return cls.test(5, 4)
a = Test()
print a.test(1, 2)
print Test.test2(3, 4)
print Test.test3()
